var countries = ee.FeatureCollection('ft:1tdSwUL7MVpOauSgRzqVTOwdfy17KDbw-1d9omPw');
var justCongo = countries.filterMetadata('Country', 'equals', 'Congo');
Map.setCenter(15.46875, -1.09857, 6);
Map.addLayer(justCongo, {'color': 'FF0000'});
