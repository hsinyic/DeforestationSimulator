var points = ee.FeatureCollection("ft:1eVBd9BT8qe323aQqH0JA-U2j9yrqdqam9-luyMta"),
    points2 = ee.FeatureCollection("ft:1RIYeOTAYfc9tBE5q3cok9d_hZ-5LY3Fsp93ERN3e");
/***** End of imports. If edited, may not auto-convert in the playground. *****/




Map.addLayer(points2);

var notdisplay=points2.filterMetadata("Production Type","equals","GAS")

Map.addLayer(notdisplay);

var display=points2.filterMetadata("Production Type","equals","OIL")

Map.addLayer(display);

Map.centerObject(points2, 3)