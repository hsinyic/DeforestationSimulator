
// Load a Fusion Table from the ID using the FeatureCollection constructor.
var op = ee.FeatureCollection('ft:10jB93qe9kDIy6OSg-UMXYrjagAB4gUKhpy6ryt45'); /*Sar_Pan: Malaysia*/
var op2 = ee.FeatureCollection('ft:1Q-5XgXQpAeRhILPoPTx8gnGTzUpSJx-EtKu_BCw'); /*Sar_Pan: Malaysia*/



// Load forest
var gfcImage = ee.Image('UMD/hansen/global_forest_change_2015');
var forest2000 = gfcImage.select(['treecover2000'])


var palmsImage = op.reduceToImage(['cat'],ee.Reducer.count());
var palmsImage2 = op2.reduceToImage(['cartodb_id'],ee.Reducer.count());


var forestWithoutPalms = forest2000.where(palmsImage.gt(0), 0);



/*
