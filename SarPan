  .filter(ee.Filter.neq('cat', null))
  .reduceToImage({
    properties: ['cat'],
    reducer: ee.Reducer.first()
});
*/

/*Map.addLayer(palmsImage, {min: 0, max: 200, palette: ['0000FF']});
*/
centerMap(13.46875, -5.09857, 8);
addToMap(forest2000)
addToMap(forestWithoutPalms)
// addToMap(palmsImage)
addToMap(op)
addToMap(op2)


