  var cumulativeCost = cost.cumulativeCost({
    source: source,
    maxDistance: 50 * 1000 // 5 kilometers
  });
  Map.addLayer(cumulativeCost, {min: 0, max: 6e3, palette:[ black, db, lb, gr, lr, yellow ] }, 'cumulativeCost2');
  var costmap = cumulativeCost
